{
  "name": "jsoql",
  "version": "0.0.24",
  "description": "Library and command line tool for querying JSON data using a SQL-like language (JSOQL)",
  "main": "jsoql.js",
  "browser": {
    "oboe": ".\\node_modules\\oboe\\dist\\oboe-node"
  },
  "author": {
    "name": "Tim Stott",
    "email": ""
  },
  "keywords": [
    "JSON",
    "SQL"
  ],
  "bin": {
    "jsoql": "cmd.js"
  },
  "scripts": {
    "test": "npm run build && cd test && ..\\node_modules\\.bin\\mocha ./ --recursive",
    "build": "npm run grammar-build && node_modules\\.bin\\tsc -p ./",
    "build-watch": "node_modules\\.bin\\tsc -p ./ -w",
    "grammar-build": "tsc grammar/parser-build.ts typings/tsd.d.ts --module commonjs && node grammar/parser-build.js",
    "develop": "node_modules\\.bin\\concurrently -k \"npm run build-watch\""
  },
  "preferGlobal": true,
  "dependencies": {
    "browser-process-hrtime": "^0.1.1",
    "buffered-xhr-stream": "^0.1.4",
    "clone": "^1.0.2",
    "csv-string": "^2.2.5",
    "csv-write-stream": "^1.0.0",
    "deep-equal": "^1.0.0",
    "glob": "^5.0.5",
    "merge": "^1.2.0",
    "moment": "^2.10.3",
    "oboe": "^2.1.1",
    "promisepipe": "^1.0.1",
    "q": "^1.2.0",
    "replacestream": "^2.1.0",
    "yargs": "^3.10.0"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "chai": "^3.2.0",
    "concurrently": "^2.0.0",
    "jison": "0.4.13",
    "mocha": "^2.2.5",
    "typescript": "^1.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tastott/jsoql"
  },
  "bugs": {
    "url": "https://github.com/tastott/jsoql/issues"
  }
}
