{
  "name": "jsoql-pad",
  "version": "0.0.13",
  "description": "GUI for running and managing JSOQL queries",
  "main": "index-nw.html",
  "scripts": {
    "start": "node_modules\\.bin\\nw",
    "start-web": "npm run browserify && node_modules\\.bin\\http-server ./ -p 8081 -o",
    "prepublish": "node prepublish.js",
    "build": "tsc -p ./",
    "browserify": "npm run build && node_modules\\.bin\\browserify app.js -o app-browser.js"
  },
  "bin": {
    "jsoql-pad": "cmd.js"
  },
  "preferGlobal": true,
  "author": "Tim Stott",
  "keywords": [
    "JSON",
    "SQL"
  ],
  "license": "ISC",
  "dependencies": {
    "angular": "^1.3.15",
    "angular-bootstrap": "^0.12.0",
    "angular-route": "^1.3.15",
    "bootstrap": "^3.3.4",
    "bootswatch": "^3.3.4",
    "brace": "^0.5.1",
    "debounce": "^1.0.0",
    "font-awesome": "^4.3.0",
    "glob": "^5.0.5",
    "jquery": "^2.1.3",
    "jsoql": "^0.0.21",
    "lazy.js": "^0.4.0",
    "nw": "^0.12.1",
    "q": "^1.2.0"
  },
  "window": {
    "width": 1200,
    "height": 800
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "browserify": "^10.2.0",
    "http-server": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tastott/jsoql"
  },
  "bugs": {
    "url": "https://github.com/tastott/jsoql/issues"
  }
}
